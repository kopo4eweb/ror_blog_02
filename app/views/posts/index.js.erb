$('#posts').append("<%= j render partial: 'delimeter' %>");
$('#posts').append("<%= j render @posts %>");

<% if @posts.next_page %>
  $('.navigation').replaceWith("<%= j will_paginate @posts, renderer: 'BootstrapPaginationHelper::LinkRenderer', class: 'navigation' %>");
<% else %>
  $('.navigation').remove();
<% end %>